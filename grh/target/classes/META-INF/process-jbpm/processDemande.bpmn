    <!-- connections -->
    <sequenceFlow id="_11-_2" sourceRef="_11" targetRef="_2" name="Soumettre" tns:priority="1" >
      <conditionExpression xsi:type="tFormalExpression" language="http://www.java.com/java" >return demande.getEtatDemande() == EtatDemande.SENT;</conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="_7-_4" sourceRef="_7" targetRef="_4" name="Proposition" tns:priority="1" >
      <conditionExpression xsi:type="tFormalExpression" language="http://www.java.com/java" >return !(demande.getEtatDemande() == EtatDemande.REJECTED);</conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="_12-_5" sourceRef="_12" targetRef="_5" />
    <sequenceFlow id="_2-_7" sourceRef="_2" targetRef="_7" />
    <sequenceFlow id="_10-_8" sourceRef="_10" targetRef="_8" />
    <sequenceFlow id="_11-_9" sourceRef="_11" targetRef="_9" name="Supprimer" tns:priority="1" >
      <conditionExpression xsi:type="tFormalExpression" language="http://www.java.com/java" >return !(demande.getEtatDemande() == EtatDemande.SENT);</conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="_1-_10" sourceRef="_1" targetRef="_10" />
    <sequenceFlow id="_7-_10" sourceRef="_7" targetRef="_10" name="Demande rejected" tns:priority="1" >
      <conditionExpression xsi:type="tFormalExpression" language="http://www.java.com/java" >return demande.getEtatDemande() == EtatDemande.REJECTED;</conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="_8-_11" sourceRef="_8" targetRef="_11" />
    <sequenceFlow id="_4-_12" sourceRef="_4" targetRef="_12" />

